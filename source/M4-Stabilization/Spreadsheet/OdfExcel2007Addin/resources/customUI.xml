<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui" onLoad="onLoad">
  <ribbon>
    <officeMenu>
      <menu id="OdfMenu" getLabel="GetLabel" insertAfterMso="FileSaveAsMenu" itemSize="large" getImage="GetImage">
        <button id="OdfImport" imageMso="FileOpen" getLabel="GetLabel" onAction="ImportODF" getEnabled="getEnabled" getDescription="GetDescription" />
        <button id="OdfExport" imageMso="FileSave" getLabel="GetLabel" onAction="ExportODF" getEnabled="getEnabled" getDescription="GetDescription" />
      </menu>
    </officeMenu>
  </ribbon>
</customUI>
